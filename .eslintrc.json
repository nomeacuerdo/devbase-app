{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "jest": true,
        "it": true,
        "xit": true,
        "describe": true,
        "beforeEach": true,
        "afterEach": true,
        "expect": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "jsx-a11y/label-has-associated-control": [ 2, {
            "assert": "both",
            "depth": 3
        }],
        "jsx-a11y/label-has-for": 0,
        "react/prop-types": 1,
        "linebreak-style": 0
    }
}
